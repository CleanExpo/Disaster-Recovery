    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXTAUTH_URL": "https://disaster-recovery-ayreio24o-unite-group.vercel.app",
    "NEXTAUTH_SECRET": "LMUDdvl91SzvH84on2RBx820GPJX33z+ICkk66EQMpg=",
    "NEXT_PUBLIC_SITE_URL": "https://disaster-recovery-ayreio24o-unite-group.vercel.app",
    "NEXT_PUBLIC_APP_URL": "https://disaster-recovery-ayreio24o-unite-group.vercel.app",
