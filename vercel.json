      "NEXT_PUBLIC_APP_URL": "https://disaster-recovery-ayreio24o-unite-group.vercel.app",
      "NEXT_PUBLIC_DEMO_MODE": "false",
      "SKIP_BUILD_ERRORS": "true"
    }
  },
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/**/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/**/route.js": {
